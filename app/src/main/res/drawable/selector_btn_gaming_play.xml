<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape>
            <gradient
                android:startColor="#f4378a"
                android:endColor="#f4378a"
                android:angle="270"/>
            <corners android:radius="21dp"/>
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <gradient
                android:startColor="#c6318f"
                android:endColor="#c6318f"
                android:angle="270"/>
            <corners android:radius="21dp"/>
        </shape>
    </item>
    <item
        android:state_focused="false"
        android:state_enabled="false">
        <shape>
            <gradient
                android:startColor="#f6a2ca"
                android:endColor="#f6a2ca"
                android:angle="270"/>
            <corners android:radius="21dp"/>
        </shape>
    </item>
</selector>
