<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape>
            <gradient
                android:startColor="#4a5050"
                android:endColor="#4a5050"
                android:angle="270"/>
            <stroke
                android:width="2dp"
                android:color="#4a5050"/>
            <corners android:radius="2dp"/>
            <padding
                android:left="10dp"
                android:top="10dp"
                android:right="10dp"
                android:bottom="10dp"/>
        </shape>
    </item>
</selector>
