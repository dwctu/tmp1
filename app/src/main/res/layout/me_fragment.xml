<?xml version="1.0" encoding="utf-8"?>
<skin.support.widget.SkinAutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="@drawable/new_title_color_bg_2"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <skin.support.widget.SkinAutoLinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <FrameLayout
            android:id="@+id/frameLayout"
            android:fitsSystemWindows="true"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:gravity="center"
                android:orientation="horizontal"
                android:paddingLeft="8dp"
                android:paddingRight="16dp"
                android:layout_width="match_parent"
                android:layout_height="44dp">
                <skin.support.widget.SkinCompatTextView
                    android:textStyle="bold"
                    android:textColor="@color/lvs_ui_standard_systemBrandGradient1"
                    android:gravity="center"
                    android:id="@+id/rl_toy_1"
                    android:background="@drawable/content_icon_pink_new"
                    android:paddingLeft="1dp"
                    android:paddingRight="1dp"
                    android:visibility="gone"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_marginLeft="8dp"
                    android:maxLines="1"
                    app:autoSizeMaxTextSize="10sp"
                    app:autoSizeMinTextSize="4sp"
                    app:autoSizeStepGranularity="1sp"
                    app:autoSizeTextType="uniform"/>
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <skin.support.widget.SkinCompatTextView
                        android:textStyle="bold"
                        android:textColor="@color/lvs_ui_standard_systemBrandGradient1"
                        android:gravity="center"
                        android:layout_gravity="bottom"
                        android:id="@+id/rl_toy_2"
                        android:background="@drawable/content_icon_pink_new"
                        android:paddingLeft="1dp"
                        android:paddingRight="1dp"
                        android:visibility="gone"
                        android:layout_width="28dp"
                        android:layout_height="28dp"
                        android:layout_marginLeft="8dp"
                        android:text="2"
                        android:maxLines="1"
                        app:autoSizeMaxTextSize="10sp"
                        app:autoSizeMinTextSize="4sp"
                        app:autoSizeStepGranularity="1sp"
                        app:autoSizeTextType="uniform"/>
                    <com.wear.widget.roundwidget.SkinRoundTextView
                        android:textSize="12dp"
                        android:textStyle="bold"
                        android:textColor="@color/lvs_ui_standard_systemBrandGradient1"
                        android:gravity="center"
                        android:id="@+id/toys_number_text"
                        android:background="@drawable/toys_number_background"
                        android:paddingLeft="@dimen/dp10"
                        android:paddingRight="@dimen/dp10"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="28dp"
                        android:textAlignment="center"/>
                </LinearLayout>
                <ImageView
                    android:id="@+id/iv_not_toy"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_marginLeft="8dp"
                    android:src="@drawable/icon_link_toy_new"/>
                <ImageView
                    android:id="@+id/title_add"
                    android:layout_width="28dp"
                    android:layout_height="28dp"
                    android:layout_marginLeft="12dp"
                    android:src="@drawable/icon_add_toy_new"/>
            </LinearLayout>
        </FrameLayout>
        <skin.support.widget.SkinAutoRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <skin.support.widget.SkinAutoLinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <skin.support.widget.SkinAutoLinearLayout
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:id="@+id/ll_account_info"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="48px">
                    <skin.support.widget.SkinAutoRelativeLayout
                        android:id="@+id/rl_account_avatar"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <de.hdodenhof.circleimageview.CircleImageView
                            android:id="@+id/riv_account_avatar"
                            android:layout_width="90dp"
                            android:layout_height="90dp"
                            android:src="@drawable/icon_default_new"
                            android:scaleType="centerCrop"/>
                        <com.wear.widget.MediumBoldTextView
                            android:textSize="36px"
                            android:textColor="@color/lvs_ui_standard_systemText"
                            android:layout_gravity="center_horizontal"
                            android:id="@+id/tv_nickname"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="24px"
                            android:text=""
                            android:layout_below="@+id/riv_account_avatar"
                            android:layout_centerHorizontal="true"
                            app:textStrokeWidth="semibold"/>
                    </skin.support.widget.SkinAutoRelativeLayout>
                    <skin.support.widget.SkinAutoLinearLayout
                        android:layout_gravity="center_horizontal"
                        android:orientation="vertical"
                        android:id="@+id/ll_nickname_available"
                        android:layout_width="wrap_content"
                        android:layout_height="match_parent">
                        <skin.support.widget.SkinAutoRelativeLayout
                            android:gravity="center_vertical"
                            android:layout_gravity="center_horizontal"
                            android:orientation="horizontal"
                            android:id="@+id/ll_available_layout"
                            android:visibility="gone"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10px">
                            <de.hdodenhof.circleimageview.CircleImageView
                                android:id="@+id/v_account_online_status"
                                android:layout_width="6dp"
                                android:layout_height="6dp"
                                android:layout_centerVertical="true"/>
                            <TextView
                                android:textSize="28px"
                                android:textColor="@color/lvs_ui_standard_systemText3"
                                android:id="@+id/tv_available"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginLeft="8px"
                                android:layout_marginRight="8px"
                                android:text="@string/user_profile_status_available"
                                android:layout_toRightOf="@+id/v_account_online_status"
                                android:layout_centerVertical="true"/>
                            <ImageView
                                android:layout_width="24px"
                                android:layout_height="24px"
                                android:src="@drawable/icon_nav_new"
                                android:layout_toRightOf="@+id/tv_available"
                                android:layout_centerVertical="true"/>
                        </skin.support.widget.SkinAutoRelativeLayout>
                    </skin.support.widget.SkinAutoLinearLayout>
                </skin.support.widget.SkinAutoLinearLayout>
                <skin.support.widget.SkinAutoLinearLayout
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="12px">
                    <skin.support.widget.SkinAutoLinearLayout
                        android:gravity="center_vertical"
                        android:id="@+id/account_setting"
                        android:background="@drawable/selector_lvs_ui_standard_card_default_bg"
                        android:paddingLeft="40px"
                        android:paddingRight="36px"
                        android:layout_width="match_parent"
                        android:layout_height="118px">
                        <ImageView
                            android:layout_width="44px"
                            android:layout_height="44px"
                            android:src="@drawable/icon_settings_new"
                            android:layout_centerVertical="true"/>
                        <skin.support.widget.SkinAutoLinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="32px">
                            <TextView
                                android:textSize="32px"
                                android:textStyle="bold"
                                android:textColor="@color/lvs_ui_standard_systemText"
                                android:gravity="center_vertical"
                                android:id="@+id/tv_setting"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/common_setting"/>
                        </skin.support.widget.SkinAutoLinearLayout>
                        <View
                            android:layout_width="0dp"
                            android:layout_height="1px"
                            android:layout_weight="1"/>
                        <View
                            android:id="@+id/me_fragment_setting_reddot"
                            android:background="@drawable/red_dot_new_bg"
                            android:visibility="visible"
                            android:layout_width="12px"
                            android:layout_height="12px"
                            android:layout_marginRight="20dp"
                            android:layout_alignParentRight="true"
                            android:layout_centerVertical="true"/>
                        <ImageView
                            android:visibility="visible"
                            android:layout_width="24px"
                            android:layout_height="24px"
                            android:src="@drawable/icon_nav_new"/>
                    </skin.support.widget.SkinAutoLinearLayout>
                    <skin.support.widget.SkinAutoLinearLayout
                        android:gravity="center_vertical"
                        android:id="@+id/account_privacy"
                        android:background="@drawable/selector_lvs_ui_standard_card_default_bg"
                        android:paddingLeft="40px"
                        android:paddingRight="36px"
                        android:layout_width="match_parent"
                        android:layout_height="118px">
                        <ImageView
                            android:layout_width="44px"
                            android:layout_height="44px"
                            android:src="@drawable/icon_privacy_new"
                            android:layout_centerVertical="true"/>
                        <TextView
                            android:textSize="32px"
                            android:textStyle="bold"
                            android:textColor="@color/lvs_ui_standard_systemText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="32px"
                            android:text="@string/common_privacy"
                            android:layout_centerVertical="true"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_height="1px"
                            android:layout_weight="1"/>
                        <LinearLayout
                            android:gravity="center_vertical"
                            android:id="@+id/me_fragment_privacy_reddot"
                            android:visibility="gone"
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent">
                            <View
                                android:background="@drawable/red_dot_new_bg"
                                android:layout_width="6dp"
                                android:layout_height="6dp"
                                android:layout_marginRight="6dp"
                                android:layout_alignParentRight="true"
                                android:layout_centerVertical="true"/>
                            <TextView
                                android:textSize="14sp"
                                android:textColor="@color/lvs_ui_standard_systemText3"
                                android:ellipsize="end"
                                android:gravity="center_vertical"
                                android:layout_width="wrap_content"
                                android:layout_height="match_parent"
                                android:layout_marginRight="8dp"
                                android:maxWidth="200dp"
                                android:text="@string/me_privacy_tutorial"
                                android:singleLine="true"/>
                        </LinearLayout>
                        <ImageView
                            android:visibility="visible"
                            android:layout_width="24px"
                            android:layout_height="24px"
                            android:src="@drawable/icon_nav_new"/>
                    </skin.support.widget.SkinAutoLinearLayout>
                    <skin.support.widget.SkinAutoLinearLayout
                        android:gravity="center_vertical"
                        android:id="@+id/account_about"
                        android:background="@drawable/selector_lvs_ui_standard_card_default_bg"
                        android:paddingLeft="40px"
                        android:paddingRight="36px"
                        android:layout_width="match_parent"
                        android:layout_height="118px">
                        <ImageView
                            android:layout_width="44px"
                            android:layout_height="44px"
                            android:src="@drawable/icon_about_new"/>
                        <skin.support.widget.SkinAutoLinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="32px">
                            <TextView
                                android:textSize="32px"
                                android:textStyle="bold"
                                android:textColor="@color/lvs_ui_standard_systemText"
                                android:id="@+id/tv_about"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="@string/common_about"/>
                            <View
                                android:id="@+id/hot_check_update"
                                android:background="@drawable/red_point_shape"
                                android:visibility="gone"
                                android:layout_width="8dp"
                                android:layout_height="8dp"
                                android:layout_marginStart="10dp"
                                android:layout_alignEnd="@+id/tv_about"/>
                        </skin.support.widget.SkinAutoLinearLayout>
                        <View
                            android:layout_width="0dp"
                            android:layout_height="1px"
                            android:layout_weight="1"/>
                        <ImageView
                            android:visibility="visible"
                            android:layout_width="24px"
                            android:layout_height="24px"
                            android:src="@drawable/icon_nav_new"/>
                    </skin.support.widget.SkinAutoLinearLayout>
                    <skin.support.widget.SkinAutoLinearLayout
                        android:gravity="center_vertical"
                        android:id="@+id/account_help"
                        android:background="@drawable/selector_lvs_ui_standard_card_default_bg"
                        android:paddingLeft="40px"
                        android:paddingRight="36px"
                        android:layout_width="match_parent"
                        android:layout_height="118px">
                        <ImageView
                            android:layout_width="44px"
                            android:layout_height="44px"
                            android:src="@drawable/icon_help_new"/>
                        <TextView
                            android:textSize="32px"
                            android:textStyle="bold"
                            android:textColor="@color/lvs_ui_standard_systemText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="32px"
                            android:text="@string/common_help"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_height="1px"
                            android:layout_weight="1"/>
                        <ImageView
                            android:visibility="visible"
                            android:layout_width="24px"
                            android:layout_height="24px"
                            android:src="@drawable/icon_nav_new"/>
                    </skin.support.widget.SkinAutoLinearLayout>
                    <skin.support.widget.SkinAutoLinearLayout
                        android:gravity="center_vertical"
                        android:id="@+id/account_logout"
                        android:background="@drawable/selector_lvs_ui_standard_card_default_bg"
                        android:paddingLeft="40px"
                        android:paddingRight="36px"
                        android:layout_width="match_parent"
                        android:layout_height="118px">
                        <ImageView
                            android:layout_width="44px"
                            android:layout_height="44px"
                            android:src="@drawable/icon_logout_new"/>
                        <TextView
                            android:textSize="32px"
                            android:textStyle="bold"
                            android:textColor="@color/lvs_ui_standard_systemText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="32px"
                            android:text="@string/account_logout"/>
                        <View
                            android:layout_width="0dp"
                            android:layout_height="1px"
                            android:layout_weight="1"/>
                    </skin.support.widget.SkinAutoLinearLayout>
                    <skin.support.widget.SkinAutoLinearLayout
                        android:orientation="horizontal"
                        android:id="@+id/layout_orgy_ingress"
                        android:visibility="invisible"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="75dp"
                        android:layout_marginTop="35dp">
                        <skin.support.widget.SkinAutoLinearLayout
                            android:gravity="top|center_horizontal"
                            android:orientation="horizontal"
                            android:id="@+id/link_orgy_web"
                            android:background="@drawable/gray_join_event"
                            android:layout_width="185dp"
                            android:layout_height="65dp">
                            <skin.support.widget.SkinAutoLinearLayout
                                android:gravity="center"
                                android:orientation="horizontal"
                                android:layout_width="match_parent"
                                android:layout_height="42dp">
                                <ImageView
                                    android:id="@+id/orgy_logo"
                                    android:layout_width="20dp"
                                    android:layout_height="20dp"
                                    android:layout_marginLeft="10dp"
                                    android:scaleType="centerCrop"/>
                                <androidx.appcompat.widget.AppCompatTextView
                                    android:textSize="16sp"
                                    android:textColor="@color/white"
                                    android:gravity="center"
                                    android:id="@+id/tv_orgy_join"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="8dp"
                                    android:layout_marginRight="10dp"
                                    android:text="Te"
                                    android:maxLines="1"
                                    app:autoSizeMaxTextSize="15sp"
                                    app:autoSizeMinTextSize="4sp"
                                    app:autoSizeStepGranularity="1sp"
                                    app:autoSizeTextType="uniform"/>
                            </skin.support.widget.SkinAutoLinearLayout>
                        </skin.support.widget.SkinAutoLinearLayout>
                        <com.wear.widget.HotView
                            android:id="@+id/hot_check_orgy"
                            android:background="@color/transparent"
                            android:layout_width="8dp"
                            android:layout_height="8dp"
                            android:layout_marginLeft="10dp"
                            android:layout_alignParentLeft="true"/>
                    </skin.support.widget.SkinAutoLinearLayout>
                </skin.support.widget.SkinAutoLinearLayout>
            </skin.support.widget.SkinAutoLinearLayout>
        </skin.support.widget.SkinAutoRelativeLayout>
    </skin.support.widget.SkinAutoLinearLayout>
</skin.support.widget.SkinAutoLinearLayout>
