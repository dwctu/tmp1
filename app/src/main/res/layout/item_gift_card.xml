<?xml version="1.0" encoding="utf-8"?>
<skin.support.constraint.SkinCompatConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/layout"
    android:background="@drawable/gift_card_item_background"
    android:layout_width="match_parent"
    android:layout_height="194dp"
    android:layout_marginLeft="16dp"
    android:layout_marginRight="16dp"
    android:layout_marginBottom="16dp">
    <View
        android:id="@+id/view1"
        android:background="@drawable/gift_card_line_view"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="47dp"
        android:layout_marginRight="17dp"
        app:layout_constraintTop_toTopOf="parent"/>
    <View
        android:id="@+id/view2"
        android:background="@drawable/gift_card_line_view"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="17dp"
        android:layout_marginTop="47dp"
        android:layout_marginRight="17dp"
        android:layout_marginBottom="50dp"
        app:layout_constraintBottom_toBottomOf="parent"/>
    <TextView
        android:textColor="@color/gift_card_item_name_color"
        android:ellipsize="marquee"
        android:id="@+id/gift_card_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="17dp"
        android:maxWidth="200dp"
        android:singleLine="true"
        app:layout_constraintBottom_toTopOf="@+id/view1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <ImageView
        android:id="@+id/giftCardImage"
        android:layout_width="90dp"
        android:layout_height="66dp"
        app:layout_constraintBottom_toBottomOf="@+id/view2"
        app:layout_constraintLeft_toLeftOf="@+id/view1"
        app:layout_constraintTop_toTopOf="@+id/view1"/>
    <TextView
        android:textColor="@color/text_color_b65_wo"
        android:ellipsize="end"
        android:id="@+id/gift_card_link"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="4dp"
        android:maxWidth="171dp"
        android:singleLine="true"
        android:textIsSelectable="true"
        app:layout_constraintLeft_toRightOf="@+id/giftCardImage"
        app:layout_constraintTop_toTopOf="@+id/giftCardImage"/>
    <TextView
        android:textColor="@color/color_FFFFFFF_85"
        android:id="@+id/gift_card_status"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        android:layout_marginBottom="4dp"
        app:layout_constraintBottom_toBottomOf="@+id/giftCardImage"
        app:layout_constraintLeft_toRightOf="@+id/giftCardImage"/>
    <skin.support.widget.SkinCompatImageView
        android:id="@+id/gift_card_share"
        android:visibility="gone"
        android:layout_width="18dp"
        android:layout_height="18dp"
        android:layout_marginRight="16dp"
        android:src="@drawable/gift_card_item_share"
        app:layout_constraintBottom_toTopOf="@+id/view1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <LinearLayout
        android:gravity="center"
        android:orientation="horizontal"
        android:id="@+id/gift_card_refund"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="16dp"
        app:layout_constraintBottom_toTopOf="@+id/view1"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <TextView
            android:textSize="14sp"
            android:textColor="@color/gift_card_item_name_color"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/button_refund"/>
        <ImageView
            android:layout_gravity="center"
            android:layout_width="@dimen/dp10"
            android:layout_height="12dp"
            android:layout_marginLeft="6dp"
            android:src="@drawable/gift_card_go"/>
    </LinearLayout>
    <TextView
        android:textColor="@color/text_color_85"
        android:id="@+id/gift_card_amount"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2"/>
    <skin.support.widget.SkinCompatImageView
        android:id="@+id/gift_card_item_copy"
        android:layout_width="15dp"
        android:layout_height="15dp"
        android:layout_marginRight="15dp"
        android:src="@drawable/gift_card_list_copy"
        app:layout_constraintBottom_toBottomOf="@+id/gift_card_link"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="@+id/gift_card_link"/>
    <LinearLayout
        android:gravity="center"
        android:orientation="horizontal"
        android:id="@+id/gift_card_noteLinear"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2">
        <TextView
            android:textSize="14sp"
            android:textColor="@color/gift_card_item_name_color"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/button_notes"/>
        <ImageView
            android:layout_gravity="center"
            android:layout_width="@dimen/dp10"
            android:layout_height="12dp"
            android:layout_marginLeft="6dp"
            android:src="@drawable/gift_card_go"/>
    </LinearLayout>
</skin.support.constraint.SkinCompatConstraintLayout>
