<?xml version="1.0" encoding="utf-8"?>
<skin.support.constraint.SkinCompatConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center"
    android:orientation="vertical"
    android:id="@+id/root_layout"
    android:background="@drawable/pattern_more_item_bg"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <LinearLayout
        android:gravity="center_vertical"
        android:id="@+id/action_row_0"
        android:visibility="visible"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        app:layout_constraintTop_toTopOf="parent">
        <ImageView
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:src="@drawable/pattern_more_rename"
            android:layout_marginStart="21dp"/>
        <TextView
            android:textSize="14dp"
            android:textColor="@color/text_color_100"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:text="@string/command_edit"/>
    </LinearLayout>
    <View
        android:id="@+id/action_row_0_line"
        android:background="@color/pattern_more_item_line_color"
        android:layout_width="0dp"
        android:layout_height="1px"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/action_row_0"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:id="@+id/action_row_1"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@+id/action_row_0">
        <ImageView
            android:id="@+id/action_row_1_icon"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:src="@drawable/copy_link_btn"
            android:layout_marginStart="21dp"/>
        <TextView
            android:textSize="14dp"
            android:textColor="@color/text_color_100"
            android:id="@+id/action_row_1_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:text="@string/button_duplicate"/>
    </LinearLayout>
    <View
        android:id="@+id/action_row_1_line"
        android:background="@color/pattern_more_item_line_color"
        android:layout_width="0dp"
        android:layout_height="1px"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/action_row_1"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:id="@+id/action_row_1_5"
        android:visibility="gone"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        app:layout_constraintTop_toBottomOf="@+id/action_row_1">
        <ImageView
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:src="@drawable/controllink_icon_reactivate_black"
            android:layout_marginStart="21dp"/>
        <TextView
            android:textSize="14dp"
            android:textColor="@color/text_color_100"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:text="@string/button_reactivate"/>
    </LinearLayout>
    <View
        android:id="@+id/action_row_1_5_line"
        android:background="@color/pattern_more_item_line_color"
        android:visibility="gone"
        android:layout_width="0dp"
        android:layout_height="1px"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/action_row_1_5"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:id="@+id/action_row_2"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/action_row_1_5">
        <ImageView
            android:id="@+id/action_row_2_icon"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:src="@drawable/controllink_icon_share2"
            android:layout_marginStart="21dp"/>
        <TextView
            android:textSize="14dp"
            android:textColor="@color/text_color_100"
            android:id="@+id/action_row_2_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:text="@string/common_share"/>
    </LinearLayout>
    <View
        android:id="@+id/action_row_2_line"
        android:background="@color/pattern_more_item_line_color"
        android:layout_width="0dp"
        android:layout_height="1px"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/action_row_2"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:id="@+id/action_row_3"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/action_row_2">
        <ImageView
            android:id="@+id/action_row_3_icon"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:src="@drawable/controllink_icon_off"
            android:layout_marginStart="21dp"/>
        <TextView
            android:textSize="14dp"
            android:textColor="@color/text_color_100"
            android:id="@+id/action_row_3_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:text="@string/button_end_session"/>
    </LinearLayout>
    <View
        android:id="@+id/action_row_3_line"
        android:background="@color/pattern_more_item_line_color"
        android:layout_width="0dp"
        android:layout_height="1px"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/action_row_3"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:id="@+id/action_row_4"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/action_row_3">
        <ImageView
            android:id="@+id/action_row_4_icon"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:src="@drawable/controllink_icon_disable"
            android:layout_marginStart="21dp"/>
        <TextView
            android:textSize="14dp"
            android:textColor="@color/text_color_100"
            android:id="@+id/action_row_4_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:text="@string/button_disable_link"/>
    </LinearLayout>
    <View
        android:id="@+id/action_row_4_line"
        android:background="@color/pattern_more_item_line_color"
        android:layout_width="0dp"
        android:layout_height="1px"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/action_row_4"/>
    <LinearLayout
        android:gravity="center_vertical"
        android:id="@+id/action_row_5"
        android:visibility="visible"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_marginTop="2dp"
        app:layout_constraintTop_toBottomOf="@+id/action_row_4">
        <ImageView
            android:id="@+id/action_row_5_icon"
            android:layout_width="18dp"
            android:layout_height="18dp"
            android:src="@drawable/pattern_more_delete"
            android:layout_marginStart="21dp"/>
        <TextView
            android:textSize="14dp"
            android:textColor="@color/text_color_100"
            android:id="@+id/action_row_5_text"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="16dp"
            android:layout_marginRight="16dp"
            android:text="@string/common_delete"/>
    </LinearLayout>
</skin.support.constraint.SkinCompatConstraintLayout>
