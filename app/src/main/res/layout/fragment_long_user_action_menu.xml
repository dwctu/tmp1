<?xml version="1.0" encoding="utf-8"?>
<com.wear.widget.roundwidget.SkinRoundAutoLinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center_horizontal"
    android:orientation="vertical"
    android:tag="layout/fragment_long_user_action_menu_0"
    android:background="@drawable/bg_bottom_dialog_systembackground"
    android:paddingLeft="32px"
    android:paddingRight="32px"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <com.wear.widget.roundwidget.SkinRoundView
        android:background="@color/lvs_ui_standard_systemText4"
        android:layout_width="80px"
        android:layout_height="8px"
        android:layout_marginTop="24px"
        app:rRadius="8px"/>
    <de.hdodenhof.circleimageview.CircleImageView
        android:tag="binding_1"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="48px"
        android:src="@drawable/ic_launcher"/>
    <com.wear.widget.roundwidget.SkinRoundTextView
        android:textSize="36px"
        android:textStyle="bold"
        android:textColor="@color/lvs_ui_standard_systemText"
        android:gravity="center"
        android:tag="binding_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24px"/>
    <com.wear.widget.roundwidget.SkinRoundAutoLinearLayout
        android:orientation="vertical"
        android:background="@color/lvs_ui_standard_systemBackground2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="64px"
        style="@style/lvs_ui_standard_radius_24px">
        <skin.support.widget.SkinAutoLinearLayout
            android:orientation="vertical"
            android:tag="binding_3"
            android:paddingLeft="40px"
            android:paddingRight="40px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.wear.widget.roundwidget.SkinRoundTextView
                android:textSize="32px"
                android:textStyle="bold"
                android:textColor="@color/lvs_ui_standard_systemText"
                android:gravity="center_vertical"
                android:id="@+id/topTop_setting"
                android:tag="binding_4"
                android:background="?android:attr/selectableItemBackground"
                android:paddingTop="37px"
                android:paddingBottom="37px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:drawablePadding="32px"/>
            <com.wear.widget.roundwidget.SkinRoundView
                android:background="@color/lvs_ui_standard_systemStrokeRegular"
                android:layout_width="match_parent"
                android:layout_height="1px"/>
        </skin.support.widget.SkinAutoLinearLayout>
        <com.wear.widget.roundwidget.SkinRoundTextView
            android:textSize="32px"
            android:textStyle="bold"
            android:textColor="@color/lvs_ui_standard_systemText"
            android:gravity="center_vertical"
            android:id="@+id/mute_notifications"
            android:tag="binding_5"
            android:background="?android:attr/selectableItemBackground"
            android:paddingLeft="40px"
            android:paddingTop="37px"
            android:paddingRight="40px"
            android:paddingBottom="37px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:drawablePadding="32px"/>
        <com.wear.widget.roundwidget.SkinRoundTextView
            android:textSize="32px"
            android:textStyle="bold"
            android:textColor="@color/lvs_ui_standard_systemText"
            android:gravity="center_vertical"
            android:background="?android:attr/selectableItemBackground"
            android:paddingLeft="40px"
            android:paddingTop="37px"
            android:paddingRight="40px"
            android:paddingBottom="37px"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Hide Conversation"
            android:drawablePadding="32px"
            android:drawableStart="@drawable/icon_long_hide_conversation"/>
        <com.wear.widget.roundwidget.SkinRoundTextView
            android:textSize="32px"
            android:textStyle="bold"
            android:textColor="@color/lvs_ui_standard_systemRedRegular"
            android:gravity="center_vertical"
            android:id="@+id/delete_group"
            android:tag="binding_6"
            android:background="?android:attr/selectableItemBackground"
            android:paddingLeft="40px"
            android:paddingTop="37px"
            android:paddingRight="40px"
            android:paddingBottom="37px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/group_chat_delete_group"
            android:drawablePadding="32px"
            android:drawableStart="@drawable/icon_long_delete_contact"/>
    </com.wear.widget.roundwidget.SkinRoundAutoLinearLayout>
    <com.wear.widget.roundwidget.SkinRoundAutoLinearLayout
        android:orientation="vertical"
        android:tag="binding_7"
        android:background="@color/lvs_ui_standard_systemBackground2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="24px"
        style="@style/lvs_ui_standard_radius_24px">
        <com.wear.widget.roundwidget.SkinRoundTextView
            android:textSize="32px"
            android:textStyle="bold"
            android:textColor="@color/lvs_ui_standard_systemText"
            android:gravity="center_vertical"
            android:id="@+id/black_setting"
            android:background="?android:attr/selectableItemBackground"
            android:paddingLeft="40px"
            android:paddingTop="37px"
            android:paddingRight="40px"
            android:paddingBottom="37px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/partner_profile_black_setting"
            android:drawablePadding="32px"
            android:drawableStart="@drawable/icon_long_block_contact"/>
    </com.wear.widget.roundwidget.SkinRoundAutoLinearLayout>
    <com.wear.widget.roundwidget.SkinRoundAutoLinearLayout
        android:orientation="vertical"
        android:background="@color/lvs_ui_standard_systemBackground2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="24px"
        android:layout_marginBottom="120px"
        style="@style/lvs_ui_standard_radius_24px">
        <skin.support.widget.SkinAutoLinearLayout
            android:orientation="vertical"
            android:tag="binding_8"
            android:paddingLeft="40px"
            android:paddingRight="40px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <com.wear.widget.roundwidget.SkinRoundTextView
                android:textSize="32px"
                android:textStyle="bold"
                android:textColor="@color/lvs_ui_standard_systemRedRegular"
                android:gravity="center_vertical"
                android:id="@+id/clear_chat_message"
                android:background="?android:attr/selectableItemBackground"
                android:paddingTop="37px"
                android:paddingBottom="37px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/clear_chat_message"
                android:drawablePadding="32px"
                android:drawableStart="@drawable/icon_long_clear_history"/>
            <com.wear.widget.roundwidget.SkinRoundView
                android:tag="binding_9"
                android:background="@color/lvs_ui_standard_systemStrokeRegular"
                android:layout_width="match_parent"
                android:layout_height="1px"/>
        </skin.support.widget.SkinAutoLinearLayout>
        <com.wear.widget.roundwidget.SkinRoundTextView
            android:textSize="32px"
            android:textStyle="bold"
            android:textColor="@color/lvs_ui_standard_systemRedRegular"
            android:gravity="center_vertical"
            android:id="@+id/delete_contact"
            android:tag="binding_10"
            android:background="?android:attr/selectableItemBackground"
            android:paddingLeft="40px"
            android:paddingTop="37px"
            android:paddingRight="40px"
            android:paddingBottom="37px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/chat_menu_delete_contact"
            android:drawablePadding="32px"
            android:drawableStart="@drawable/icon_long_delete_contact"/>
        <com.wear.widget.roundwidget.SkinRoundTextView
            android:textSize="32px"
            android:textStyle="bold"
            android:textColor="@color/lvs_ui_standard_systemRedRegular"
            android:gravity="center_vertical"
            android:id="@+id/leave_group"
            android:tag="binding_11"
            android:background="?android:attr/selectableItemBackground"
            android:paddingLeft="40px"
            android:paddingTop="37px"
            android:paddingRight="40px"
            android:paddingBottom="37px"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/group_chat_setting_exit"
            android:drawablePadding="32px"
            android:drawableStart="@drawable/icon_long_leave_group"/>
    </com.wear.widget.roundwidget.SkinRoundAutoLinearLayout>
</com.wear.widget.roundwidget.SkinRoundAutoLinearLayout>
