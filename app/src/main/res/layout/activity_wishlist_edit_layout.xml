<?xml version="1.0" encoding="utf-8"?>
<skin.support.constraint.SkinCompatConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <skin.support.constraint.SkinCompatConstraintLayout
        android:background="@color/bg"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/app_bar_layout"
            android:layout_width="match_parent"
            android:layout_height="44dp"
            app:layout_constraintTop_toTopOf="parent">
            <ImageView
                android:id="@+id/nav_back"
                android:background="@color/transparent"
                android:layout_width="wrap_content"
                android:layout_height="22dp"
                android:src="@drawable/nav_back_selector"
                android:paddingStart="16dp"
                android:paddingEnd="16dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
            <TextView
                android:textSize="18sp"
                android:textColor="@color/text_color_85"
                android:id="@+id/edit_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/command_edit"
                android:layout_marginEnd="16dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.swiperefreshlayout.widget.SwipeRefreshLayout
            android:id="@+id/refresh_layout_view"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/app_bar_layout">
            <ScrollView
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <ImageView
                        android:id="@+id/profile_view"
                        android:layout_width="60dp"
                        android:layout_height="60dp"
                        android:layout_marginTop="21dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"/>
                    <TextView
                        android:textSize="18sp"
                        android:textStyle="bold"
                        android:textColor="@color/text_color_85"
                        android:gravity="center"
                        android:id="@+id/wishlist_name_view"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:paddingStart="25dp"
                        android:paddingEnd="25dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/profile_view"/>
                    <TextView
                        android:textSize="14sp"
                        android:textColor="@color/wish_list_description_text_color"
                        android:gravity="center"
                        android:id="@+id/wishlist_description_view"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="8dp"
                        android:paddingStart="25dp"
                        android:paddingEnd="25dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/wishlist_name_view"/>
                    <skin.support.constraint.SkinCompatConstraintLayout
                        android:id="@+id/contribution_bar"
                        android:background="@drawable/wish_list_notice_bar_drawable"
                        android:paddingTop="10dp"
                        android:paddingBottom="10dp"
                        android:visibility="gone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:paddingStart="8dp"
                        android:paddingEnd="8dp"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/wishlist_description_view">
                        <RelativeLayout
                            android:id="@+id/num_background"
                            android:background="@drawable/cycle_background"
                            android:padding="1dp"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:minWidth="20dp"
                            android:minHeight="20dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent">
                            <TextView
                                android:textSize="12sp"
                                android:textColor="@color/white"
                                android:id="@+id/num_contribute_view"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="99+"
                                android:layout_centerInParent="true"/>
                        </RelativeLayout>
                        <TextView
                            android:textColor="@color/wish_list_link_text_color"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/wish_list_new_contributions"
                            android:layout_marginStart="8dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            app:layout_constraintStart_toEndOf="@+id/num_background"
                            app:layout_constraintTop_toTopOf="parent"/>
                    </skin.support.constraint.SkinCompatConstraintLayout>
                    <skin.support.constraint.SkinCompatConstraintLayout
                        android:id="@+id/link_item_layout"
                        android:background="@drawable/wish_list_item_background_drawable"
                        android:padding="20dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="24dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginEnd="16dp"
                        app:layout_constraintTop_toBottomOf="@+id/contribution_bar">
                        <com.wear.widget.MediumBoldTextView
                            android:textColor="@color/text_color_group_manager"
                            android:id="@+id/link_title"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="4dp"
                            android:text="@string/wish_list_link_title"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"
                            app:textStrokeWidth="semibold"/>
                        <skin.support.constraint.SkinCompatConstraintLayout
                            android:id="@+id/link_layout"
                            android:background="@drawable/link_background_drawable"
                            android:paddingTop="13dp"
                            android:paddingBottom="13dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="16dp"
                            android:paddingStart="16dp"
                            android:paddingEnd="16dp"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/link_title">
                            <com.wear.widget.MediumBoldTextView
                                android:textSize="15sp"
                                android:textColor="@color/wish_list_link_text_color"
                                android:gravity="start"
                                android:id="@+id/link_text"
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:singleLine="true"
                                android:layout_marginEnd="20dp"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toStartOf="@+id/copy_btn"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"
                                app:textStrokeWidth="semibold"/>
                            <ImageView
                                android:id="@+id/copy_btn"
                                android:background="@drawable/copy_link_btn"
                                android:layout_width="30dp"
                                android:layout_height="30dp"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toTopOf="parent"/>
                        </skin.support.constraint.SkinCompatConstraintLayout>
                        <Button
                            android:textColor="@color/white"
                            android:id="@+id/share_wish_list_btn"
                            android:background="@drawable/wish_list_pink_btn_drawable"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="20dp"
                            android:text="@string/button_share_wish_list"
                            app:layout_constraintTop_toBottomOf="@+id/link_layout"/>
                    </skin.support.constraint.SkinCompatConstraintLayout>
                    <skin.support.constraint.SkinCompatConstraintLayout
                        android:id="@+id/toys_layout"
                        android:background="@drawable/wish_list_item_background_drawable"
                        android:paddingTop="26dp"
                        android:paddingBottom="24dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:paddingStart="22dp"
                        android:paddingEnd="22dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginEnd="16dp"
                        app:layout_constraintTop_toBottomOf="@+id/link_item_layout">
                        <ImageView
                            android:id="@+id/wish_list_icon_view"
                            android:layout_width="17dp"
                            android:layout_height="17dp"
                            android:src="@drawable/wish_list_icon"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <com.wear.widget.MediumBoldTextView
                            android:textSize="14sp"
                            android:textColor="@color/text_color_group_manager"
                            android:id="@+id/wish_list_text"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/discover_wish_list"
                            android:singleLine="true"
                            android:layout_marginStart="14dp"
                            android:layout_marginEnd="20dp"
                            app:layout_constraintStart_toEndOf="@+id/wish_list_icon_view"
                            app:layout_constraintTop_toTopOf="parent"
                            app:textStrokeWidth="semibold"/>
                        <androidx.recyclerview.widget.RecyclerView
                            android:id="@+id/toys_recycler_view"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="16dp"
                            android:overScrollMode="never"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="@+id/wish_list_text"
                            app:layout_constraintTop_toBottomOf="@+id/wish_list_icon_view"/>
                    </skin.support.constraint.SkinCompatConstraintLayout>
                    <skin.support.constraint.SkinCompatConstraintLayout
                        android:id="@+id/fan_layout"
                        android:background="@drawable/wish_list_item_background_drawable"
                        android:paddingTop="26dp"
                        android:paddingBottom="24dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="20dp"
                        android:paddingStart="22dp"
                        android:paddingEnd="22dp"
                        android:layout_marginStart="16dp"
                        android:layout_marginEnd="16dp"
                        app:layout_constraintTop_toBottomOf="@+id/toys_layout">
                        <ImageView
                            android:id="@+id/fund_icon_view"
                            android:layout_width="17dp"
                            android:layout_height="17dp"
                            android:src="@drawable/fund_icon"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toTopOf="parent"/>
                        <com.wear.widget.MediumBoldTextView
                            android:textSize="14sp"
                            android:textColor="@color/text_color_group_manager"
                            android:gravity="start"
                            android:id="@+id/fund_text_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/wish_list_fan_funding"
                            android:singleLine="true"
                            android:layout_marginStart="14dp"
                            android:layout_marginEnd="20dp"
                            app:layout_constraintStart_toEndOf="@+id/fund_icon_view"
                            app:layout_constraintTop_toTopOf="parent"
                            app:textStrokeWidth="semibold"/>
                        <TextView
                            android:textSize="15sp"
                            android:textColor="@color/wish_list_description_text_color"
                            android:id="@+id/fund_description"
                            android:layout_width="0dp"
                            android:layout_height="wrap_content"
                            android:singleLine="true"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="@+id/fund_text_view"
                            app:layout_constraintTop_toBottomOf="@+id/fund_text_view"/>
                        <include
                            android:id="@+id/fund_toy_view"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="16dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            app:layout_constraintStart_toStartOf="parent"
                            app:layout_constraintTop_toBottomOf="@+id/fund_description"
                            layout="@layout/wish_list_toy_item"/>
                        <skin.support.constraint.SkinCompatConstraintLayout
                            android:id="@+id/progress_bar"
                            android:background="@drawable/wish_list_fund_progress_background"
                            android:layout_width="match_parent"
                            android:layout_height="5dp"
                            android:layout_marginTop="24dp"
                            app:layout_constraintTop_toBottomOf="@+id/fund_toy_view">
                            <ImageView
                                android:id="@+id/progress_number"
                                android:background="@drawable/clip_drawable_wish_list_pregress"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"/>
                        </skin.support.constraint.SkinCompatConstraintLayout>
                        <androidx.constraintlayout.widget.ConstraintLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/progress_bar">
                            <TextView
                                android:textSize="15sp"
                                android:textColor="@color/wish_list_people_contribution_text_color"
                                android:id="@+id/people_contribution_text_view"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintStart_toStartOf="parent"
                                app:layout_constraintTop_toTopOf="parent"/>
                            <TextView
                                android:textSize="15sp"
                                android:textColor="@color/wish_list_people_contribution_text_color"
                                android:id="@+id/percent_text_view"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                app:layout_constraintBottom_toBottomOf="parent"
                                app:layout_constraintEnd_toEndOf="parent"
                                app:layout_constraintTop_toTopOf="parent"/>
                        </androidx.constraintlayout.widget.ConstraintLayout>
                    </skin.support.constraint.SkinCompatConstraintLayout>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </ScrollView>
        </androidx.swiperefreshlayout.widget.SwipeRefreshLayout>
    </skin.support.constraint.SkinCompatConstraintLayout>
</skin.support.constraint.SkinCompatConstraintLayout>
