<?xml version="1.0" encoding="utf-8"?>
<skin.support.widget.SkinAutoRelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:bar="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/root_layout"
    android:background="@color/lvs_ui_standard_systemBackground"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <skin.support.widget.SkinAutoFrameLayout
        android:id="@+id/login_top_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <ImageView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/test_2"
            android:adjustViewBounds="true"/>
        <skin.support.widget.SkinAutoRelativeLayout
            android:layout_width="match_parent"
            android:layout_height="88px"
            android:layout_marginTop="110px">
            <ImageView
                android:id="@+id/actionbar_back"
                android:background="@color/transparent"
                android:layout_width="44px"
                android:layout_height="44px"
                android:layout_marginLeft="32px"
                android:src="@drawable/nav_back_selector"
                android:layout_centerVertical="true"/>
            <com.wear.widget.MediumBoldTextView
                android:textSize="18sp"
                android:textStyle="bold"
                android:textColor="@color/text_color_85"
                android:gravity="center"
                android:id="@+id/actionbar_title"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/welcome_signup"
                android:singleLine="true"
                android:layout_centerInParent="true"/>
        </skin.support.widget.SkinAutoRelativeLayout>
        <com.wear.widget.MyActionBar
            android:id="@+id/actionbar"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="48dp"
            android:src="@drawable/test_2"
            bar:back="true"
            bar:title="@string/welcome_signup"/>
    </skin.support.widget.SkinAutoFrameLayout>
    <skin.support.widget.SkinAutoRelativeLayout
        android:orientation="vertical"
        android:paddingLeft="80px"
        android:paddingRight="80px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:textSize="40px"
            android:textStyle="bold"
            android:textColor="@color/text_color_85"
            android:id="@+id/sign_up_des"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="273px"
            android:text="@string/sign_up_des"/>
        <skin.support.widget.SkinAutoRelativeLayout
            android:orientation="horizontal"
            android:id="@+id/nickname_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="444px">
            <EditText
                android:textSize="32px"
                android:textColor="@color/text_color_100"
                android:textColorHint="@color/text_color_25"
                android:id="@+id/nickname"
                android:background="@color/transparent"
                android:padding="5px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/signup_name_hint"
                android:singleLine="true"/>
            <ImageView
                android:id="@+id/nickname_content_delete"
                android:visibility="gone"
                android:layout_width="36px"
                android:layout_height="36px"
                android:src="@drawable/icon_psw_clear"
                android:layout_alignParentRight="true"/>
            <View
                android:id="@+id/view"
                android:background="@color/lvs_ui_standard_systemStrokeRegular"
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginTop="33px"
                android:layout_below="@+id/nickname"/>
            <TextView
                android:textSize="24px"
                android:textColor="@color/top_tip_text_color"
                android:id="@+id/tv_name_notice"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10px"
                android:text="@string/signup_name_notice"
                android:layout_below="@+id/view"/>
        </skin.support.widget.SkinAutoRelativeLayout>
        <skin.support.widget.SkinAutoRelativeLayout
            android:id="@+id/email_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="587px">
            <EditText
                android:textSize="32px"
                android:textColor="@color/text_color_100"
                android:textColorHint="@color/text_color_25"
                android:id="@+id/email"
                android:background="@color/transparent"
                android:padding="5px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/common_email"
                android:singleLine="true"/>
            <ImageView
                android:id="@+id/email_content_delete"
                android:visibility="gone"
                android:layout_width="36px"
                android:layout_height="36px"
                android:src="@drawable/icon_psw_clear"
                android:layout_alignParentRight="true"/>
            <View
                android:id="@+id/view1"
                android:background="@color/lvs_ui_standard_systemStrokeRegular"
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginTop="33px"
                android:layout_below="@+id/email"/>
            <TextView
                android:textSize="24px"
                android:textColor="@color/top_tip_text_color"
                android:id="@+id/tv_email_notice"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10px"
                android:text="@string/signup_email_notice"
                android:layout_below="@+id/view1"/>
        </skin.support.widget.SkinAutoRelativeLayout>
        <skin.support.widget.SkinAutoRelativeLayout
            android:id="@+id/password_layout"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="730px">
            <EditText
                android:textSize="32px"
                android:textColor="@color/text_color_100"
                android:textColorHint="@color/text_color_25"
                android:id="@+id/password"
                android:background="@color/transparent"
                android:padding="5px"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="@string/common_password"
                android:singleLine="true"
                android:inputType="textPassword"
                android:imeOptions="actionSend"/>
            <ImageView
                android:id="@+id/password_content_show"
                android:visibility="gone"
                android:layout_width="36px"
                android:layout_height="36px"
                android:src="@drawable/icon_eye_close"
                android:layout_alignParentRight="true"/>
            <ImageView
                android:id="@+id/password_content_delete"
                android:visibility="gone"
                android:layout_width="33px"
                android:layout_height="33px"
                android:layout_marginRight="65px"
                android:src="@drawable/icon_psw_clear"
                android:layout_toLeftOf="@+id/password_content_show"
                android:layout_alignParentRight="true"/>
            <View
                android:id="@+id/view2"
                android:background="@color/lvs_ui_standard_systemStrokeRegular"
                android:layout_width="match_parent"
                android:layout_height="1px"
                android:layout_marginTop="33px"
                android:layout_below="@+id/password"/>
            <TextView
                android:textSize="24px"
                android:textColor="@color/top_tip_text_color"
                android:id="@+id/tv_password_notice"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10px"
                android:text="@string/signup_password_notice"
                android:layout_below="@+id/view2"/>
        </skin.support.widget.SkinAutoRelativeLayout>
        <com.wear.widget.roundwidget.SkinRoundTextView
            android:enabled="false"
            android:textSize="32px"
            android:textColor="@color/text_white_login_selector"
            android:gravity="center"
            android:id="@+id/signUp_btn"
            android:visibility="visible"
            android:clickable="true"
            android:layout_width="match_parent"
            android:layout_height="88px"
            android:layout_marginTop="921px"
            android:text="@string/welcome_signup"
            style="@style/lvs_ui_standard_brand_gradient_radius_20px"/>
        <skin.support.widget.SkinAutoRelativeLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="48px"
            android:layout_below="@+id/signUp_btn">
            <CheckBox
                android:id="@+id/privacy_cbox"
                android:background="@color/color_primary_dark"
                android:visibility="gone"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:checked="true"
                style="@style/PolicyCheckboxTheme"/>
            <TextView
                android:textSize="12sp"
                android:textColor="@color/text_color_45"
                android:id="@+id/web_link"
                android:padding="2dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="2dp"
                android:text="@string/signup_license_privacy"
                android:layout_toRightOf="@+id/privacy_cbox"/>
        </skin.support.widget.SkinAutoRelativeLayout>
    </skin.support.widget.SkinAutoRelativeLayout>
</skin.support.widget.SkinAutoRelativeLayout>
